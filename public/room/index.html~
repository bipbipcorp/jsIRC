                  <script src="../jquery.titlealert.min.js"></script>
                  <script src="app.js"></script>
                  <script src="client.js"></script>

                  <script>
                           // Create instance of app
                           var application = new app();

                           // Dynamically load the socket.io file
                           $.ajax( {
                                    url: Config.Client.ServerUrl + "socket.io/socket.io.js"
                                    ,dataType: "script"
                                    // And init the ui, upon success
                                    ,success: application.init.bind( application )
                           } );
                  </script>

